{
  "Sources": [
    {
      "Type": "Mqtt",
      "Server": "192.168.178.20",
      "Port": 1883,
      "Topics": {
        "Result": "cmnd/sonoff-0925-dimmer-3/color"
      },
      "Transformations": [
        {
          "Type": "JsonPath",
          "Expression": "$.Color"
        },
        {
          "Type": "Color",
          "ChannelLayout": "_RGB"
        }
      ]
    }
  ],
  "Sinks": [
    {
      "Type": "OpenRGB",
      "Server": "192.168.178.120",
      "Port": 6742,
      "Overrides": {
        "MSI Mystic Light MS_7C84": {
          "Zones": {
            "JRGB2": {
              "ChannelLayout": "GRB"
            }
          }
        }
      },
      "Transformations": [
        {
          "Type": "Expression",
          "Expression": "value.B == value.R && value.R == value.G ? \"White.orp\" : (value.B > value.R && value.B > value.G ? \"Purple.orp\" : (value.G > value.B && value.G > value.R ? \"Green.orp\" : \"Red.orp\"))"
        }
      ]
    }
  ]
}